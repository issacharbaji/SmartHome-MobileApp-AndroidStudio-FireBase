        app:layout_constraintEnd_toEndOf="@+id/masterBedRoomAc"
        app:layout_constraintHorizontal_bias="0.226"
        app:layout_constraintStart_toStartOf="@+id/masterBedRoomAc"
        app:layout_constraintTop_toTopOf="@+id/masterBedRoomAc"
        app:layout_constraintVertical_bias="0.819" />

    <TextView
        android:id="@+id/temperature1"
        android:layout_width="42dp"
        android:layout_height="40dp"
        android:background="@drawable/temperature"
        android:paddingLeft="10dp"
        android:paddingTop="7dp"
        android:text="18"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/masterBedRoomAc"
        app:layout_constraintEnd_toEndOf="@+id/masterBedRoomAc"
        app:layout_constraintHorizontal_bias="0.748"
        app:layout_constraintStart_toStartOf="@+id/masterBedRoomAc"
        app:layout_constraintTop_toTopOf="@+id/masterBedRoomAc"
        app:layout_constraintVertical_bias="0.819" />








    <Button
