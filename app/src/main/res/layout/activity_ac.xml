        app:layout_constraintHorizontal_bias="0.426"
        app:layout_constraintStart_toStartOf="@+id/kidsBedRoomAc"
        app:layout_constraintTop_toTopOf="@+id/kidsBedRoomAc"
        app:layout_constraintVertical_bias="0.819" />

