        app:layout_constraintStart_toStartOf="@+id/masterBedRoomAc"
        app:layout_constraintTop_toTopOf="@+id/masterBedRoomAc"
        app:layout_constraintVertical_bias="0.367" />

    <Button
        android:id="@+id/buttonUp1"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/acbuttons"
        android:drawableTop="@drawable/ic_arrow_upward_black_24dp"
